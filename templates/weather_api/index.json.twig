{
    "country": "{{ country }}",
    "city": "{{ city }}",
    "measurements": [
        {% for measurement in measurements %}
        {
            "date": "{{ measurement.date | date('Y-m-d')  }}",
            "celsius": "{{ measurement.getCelsius() }}",
            "fahrenheit": "{{ measurement.getFahrenheit() }}"
        }{% if not loop.last %},{% endif %}
        {% endfor %}
    ]
}
